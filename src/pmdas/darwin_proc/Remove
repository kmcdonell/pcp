# Remove the Darwin per-process (proc) PMDA
#

. $PCP_DIR/etc/pcp.env
. $PCP_SHARE_DIR/lib/pmdaproc.sh

iam=proc
pmns_name="proc hotproc"

pmdaSetup
pmdaRemove
exit
